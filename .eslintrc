{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "restParams": true,
    "spread": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true
  },
  "env": {
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "consistent-return": 2,
    "curly": 2,
    "dot-location": [
      2,
      "property"
    ],
    "handle-callback-err": 2,
    "no-caller": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-else-return": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-inner-declarations": 0,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-param-reassign": [
      2,
      {
        "props": true
      }
    ],
    "no-path-concat": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-sync": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-unexpected-multiline": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-var": 2,
    "no-void": 2,
    "no-with": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "prefer-reflect": 0,
    "prefer-template": 2,
    "radix": 2,
    "require-yield": 2,
    "valid-jsdoc": 0,
    "yoda": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "block-spacing": [
      2,
      "always"
    ],
    "brace-style": [
      1,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": 2,
    "comma-spacing": 2,
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "eol-last": 2,
    "func-style": 0,
    "generator-star-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "indent": [
      2,
      2
    ],
    "key-spacing": 2,
    "linebreak-style": 2,
    "max-nested-callbacks": 0,
    "new-cap": 2,
    "new-parens": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-nested-ternary": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [
      2,
      "never"
    ],
    "operator-linebreak": [
      2,
      "before"
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "quotes": [
      2,
      "double"
    ],
    "semi-spacing": 2,
    "semi": [
      2,
      "always"
    ],
    "space-after-keywords": 2,
    "space-before-blocks": 1,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2
  },
  "globals": {
    "super": true
  }
}
